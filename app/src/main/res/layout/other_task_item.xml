<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/main_bg">

    <androidx.cardview.widget.CardView
            android:id="@+id/other_task_item_cv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="6dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="6dp"
            android:layout_marginBottom="12dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_title_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:text="保养任务"
                    android:textColor="@color/main_button_bg"
                    android:textSize="@dimen/text_size_16"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_type_name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="30dp"
                    android:text="保养中"
                    android:textColor="@color/main_button_bg"
                    android:textSize="@dimen/text_size_12"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_line"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="12dp"
                    android:background="@color/sin_in_text_line_color"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_title_tag"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_code_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="电梯注册代码:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_line"
                    tools:ignore="MissingConstraints" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_code_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_code_tag"
                    tools:ignore="MissingConstraints" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_unit_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="使用单位:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_line"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_unit_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_unit_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_code_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_name_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电梯名称:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_name_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_name_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_unit_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_address_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电梯地址:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_address_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_address_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_name_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_type_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电梯类型:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_type_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_type_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_address_tv"
                    tools:ignore="MissingConstraints" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_number_tag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电梯编码:"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toLeftOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_title_tag"
                    app:layout_constraintTop_toTopOf="@id/other_task_item_number_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_number_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="20dp"

                    android:maxLines="2"
                    android:text="858635852422352525522522525522555"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size_14"
                    app:layout_constraintLeft_toRightOf="@id/other_task_item_code_tag"
                    app:layout_constraintRight_toRightOf="@id/other_task_item_line"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_type_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/other_task_item_0_line"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginRight="12dp"
                    android:visibility="gone"
                    android:background="@color/sin_in_text_line_color"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_number_tv"
                    tools:ignore="MissingConstraints" />


            <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/other_task_item_btn"
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_width="100dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="20dp"
                    android:visibility="gone"
                    app:layout_goneMarginBottom="20dp"
                    android:layout_marginRight="12dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/main_btn_bg"
                    android:foreground="?attr/selectableItemBackground"
                    android:text="立即保养"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/other_task_item_0_line"
                    tools:ignore="MissingConstraints" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</FrameLayout>